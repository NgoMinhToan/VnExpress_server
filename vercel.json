{
    "builds": [
        {
            "src": "src/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/wsgi.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 10 * * *"
        }
    ]
}